{
  "author": "bfrancoisjr",
  "private": true,
  "description": "TypeScript + Express API Server",
  "engines": {
    "node": "20.x || 21.x || 22.x || 23.x || 24.x"
  },
  "license": "ISC",
  "name": "francoisrentals-api",
  "scripts": {
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rimraf dist coverage",
    "clean:logs": "rimraf logs",
    "debug": "NODE_ENV=development nodemon --inspect src/server.ts",
    "deploy:dev": "pm2 start ecosystem.config.js --only fr-dev",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only fr-prod",
    "dev": "cross-env NODE_ENV=development node dist/server.js",
    "dev:local": "cross-env NODE_ENV=development nodemon",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "docker:net:create": "docker network create fr-net || true",
    "frdb:docker:up": "docker run --name fr-pg-db --network fr-net -e POSTGRES_PASSWORD=mysecretpassword -d -p 5432:5432 -v pg_data:/var/lib/postgresql/data postgres",
    "frdb:docker:down": "docker stop fr-pg-db && docker rm fr-pg-db",
    "start:docker": "docker run --name fr-api -d --rm -p 3000:3000 --network fr-net -e DB_HOST=fr-pg-db -e DB_PORT=5432 -e DB_USER=postgres -e DB_PASSWORD=mysecretpassword -e DB_DATABASE=postgres fr-api:dev",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test": "npm run test:unit && npm run test:int",
    "test:int": "NODE_ENV=test jest -c jest.integration.config.ts --runInBand --no-cache --detectOpenHandles --forceExit",
    "test:unit": "jest -c jest.config.ts --detectOpenHandles --runInBand",
    "test:unit:coverage": "jest -c jest.config.ts --detectOpenHandles --runInBand --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.948.0",
    "@aws-sdk/s3-request-presigner": "3.948.0",
    "axios": "1.13.2",
    "bcrypt": "6.0.0",
    "cheerio": "1.1.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.3",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "envalid": "8.1.1",
