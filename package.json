{
  "author": "bfrancoisjr",
  "private": true,
  "description": "TypeScript + Mongoose + MongoDB + Express API Server",
  "engines": {
    "node": "18.x || 19.x"
  },
  "license": "ISC",
  "name": "nba-stats-comparison",
  "scripts": {
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rimraf dist coverage",
    "clean:logs": "rimraf logs",
    "debug": "NODE_ENV=development nodemon --inspect src/server.ts",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "dev": "cross-env NODE_ENV=development nodemon",
    "heroku:local": "heroku local -e .env.development.local ",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "mongodb:up": "mongod --dbpath data",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "test:unit": "jest -c jest.config.ts --detectOpenHandles",
    "test:int": "jest -c jest.integration.config.ts",
    "test": "npm run test:unit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.367.0",
    "@google-cloud/local-auth": "^2.1.1",
    "axios": "^1.3.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "envalid": "^7.1.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.4",
    "google-auth-library": "^8.7.0",
    "googleapis": "^111.0.0",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.9.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "swagger-jsdoc": "^6.0.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^4.10.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.38",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.1",
    "@types/jest": "27.4.x",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/mongoose": "^5.10.1",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.14.6",
    "@types/pdf-parse": "^1.1.1",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest": "27.4.x",
    "lint-staged": "^11.0.0",
    "mongodb-memory-server": "^8.12.2",
    "multer-s3": "^3.0.1",
    "node-config": "^0.0.2",
    "node-gyp": "^8.1.0",
    "nodemon": "^2.0.9",
    "pm2": "^5.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^4.4.1",
    "semantic-release": "^21.0.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.4.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.5.2"
  },
  "config": {
    "mongodbMemoryServer": {
      "systemBinary": "/opt/homebrew/bin/mongod",
      "arch": "x64",
      "disablePostinstall": "1",
      "version": "6.0.5",
      "debug": "0"
    }
  },
  "release": {
    "repositoryUrl": "https://github.com/francobb/francoisrentals-api",
    "branches": [
      "main"
    ]
  }
}
