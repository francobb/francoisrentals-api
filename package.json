{
  "author": "bfrancoisjr",
  "private": true,
  "description": "TypeScript + Mongoose + MongoDB + Express API Server",
  "engines": {
    "node": "18.x || 19.x || 20.x"
  },
  "license": "ISC",
  "name": "francoisrentals-api",
  "scripts": {
    "build": "swc src -d dist --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rimraf dist coverage",
    "clean:logs": "rimraf logs",
    "debug": "NODE_ENV=development nodemon --inspect src/server.ts",
    "deploy:dev": "pm2 start ecosystem.config.js --only fr-dev",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only fr-prod",
    "dev": "cross-env NODE_ENV=development node dist/server.js",
    "dev:local": "./scripts/mongodb-test.sh && cross-env NODE_ENV=development nodemon",
    "heroku:local": "heroku local -e .env.development.local ",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "frdb:docker:down": "docker stop fr-db",
    "frdb:docker:up": "docker run --name fr-db -d --rm --network fr-net -v data:/data/db -p 27017:27017 mongo",
    "frdb:local:down": "pkill mongod || true",
    "frdb:local:up": "mongod --dbpath data  --port 27017",
    "start:docker": "docker run --name fr-api -d --rm -p 3000:3000 --network fr-net  -e MONGO_URI='mongodb://fr-db:27017/dev' fr-api:dev",
    "start": "cross-env NODE_ENV=development node dist/server.js",
    "test": "npm run test:unit && npm run test:int",
    "test:int": "./scripts/mongodb-test.sh && NODE_ENV=test jest -c jest.integration.config.ts --runInBand --no-cache --detectOpenHandles --forceExit && npm run frdb:local:down",
    "test:unit": "jest -c jest.config.ts --detectOpenHandles --runInBand",
    "test:unit:coverage": "jest -c jest.config.ts --detectOpenHandles --runInBand --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.474.0",
    "@aws-sdk/s3-request-presigner": "3.529.1",
    "axios": "1.7.2",
    "bcrypt": "5.1.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "10.0.0",
    "envalid": "7.3.1",
    "express": "4.19.2",
    "express-favicon": "2.0.4",
    "express-session": "1.18.0",
    "firebase": "10.12.4",
    "firebase-admin": "12.3.0",
    "googleapis": "111.0.0",
    "helmet": "4.6.0",
    "hpp": "0.2.3",
    "jsonwebtoken": "9.0.2",
    "keycloak-connect": "25.0.0",
    "mongoose": "6.12.3",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "node-cron": "3.0.3",
    "nodemailer": "6.9.9",
    "passport": "0.6.0",
    "passport-google-oauth20": "2.0.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pdf-parse": "1.1.1",
    "stripe": "12.18.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.3",
    "twilio": "4.20.0",
    "winston": "3.11.0",
    "winston-daily-rotate-file": "4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "0.1.63",
    "@swc/core": "1.3.100",
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/cookie-parser": "1.4.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
